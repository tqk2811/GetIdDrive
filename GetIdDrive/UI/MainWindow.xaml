<Window x:Class="GetIdDrive.UI.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GetIdDrive.UI"
        xmlns:ViewModels="clr-namespace:GetIdDrive.UI.ViewModels"
        mc:Ignorable="d"
        Title="MainWindow" Height="127.53" Width="429.899" WindowStartupLocation="CenterScreen">
  <Window.DataContext>
    <ViewModels:MainWindowViewModel></ViewModels:MainWindowViewModel>
  </Window.DataContext>
  <Grid>
    <TextBox Height="20" Margin="10,10,10,0" TextWrapping="NoWrap" Text="{Binding ParentId}" VerticalAlignment="Top"/>
    <Button x:Name="btn_GetChildsId" Click="btn_GetChildsId_Click" Content="Lấy danh sách file" HorizontalAlignment="Left" Margin="10,35,0,0" VerticalAlignment="Top" Width="115"/>

  </Grid>
</Window>
